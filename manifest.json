{
    "name": "Go Link Plugin",
    "description": "Use go{space} link to any url. Built for xGoogler. Map stored in your google account.",
    "version": "1.0.0",
    "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "manifest_version": 3,
    "action": {
      "default_popup": "main.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "omnibox": {
      "keyword": "go"
    },
    "background": {
      "service_worker": "background.js"
    }
  }