{
    "name": "Go Link Plugin",
    "description": "Use go/ link to any url.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "main.html",
      "default_icon": "icon.png"
    },
    "permissions": [
      "tabs",
      "storage"
    ],
    "host_permissions": ["<all_urls>"],
    "omnibox": {
      "keyword": "go"
    },
    "background": {
      "service_worker": "background.js"
    }
  }